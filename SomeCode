public class Pilot {
    int age;
    String name;
    Aircraft boeing;


    public int getAge() {
        return age;
    }


    public void setAge(int age) {
        this.age = age;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public void goLanding(){
        boeing.landingPosition();
        System.out.println("Weight on wheel");
    }
    public void goTakeOff(){
        boeing.takeOffPosition();
        System.out.println("Weight off wheel");
    }

    public void getSkill(Aircraft boeing){
        this.boeing = boeing;
    }
}
